{
    "name": "oqq/pact",
    "description": "TBD",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "pact",
        "contract-testing"
    ],
    "authors": [
        {
            "name": "Eric Braun",
            "email": "eb@oqq.be"
        }
    ],
    "require": {
        "php": "8.3.* || 8.4.*",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "infection/infection": "^0.29",
        "phpunit/phpunit": "^12.0",
        "psalm/plugin-phpunit": "^0.19",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^6.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": false
        }
    },
    "autoload": {
        "psr-4": {
            "Oqq\\Pact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oqq\\PactTest\\": "tests/"
        }
    }
}
