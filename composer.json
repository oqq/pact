{
    "name": "oqq/pact",
    "description": "TBD",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "pact",
        "contract-testing"
    ],
    "authors": [
        {
            "name": "Eric Braun",
            "email": "eb@oqq.be"
        }
    ],
    "require": {
        "php": "8.0.* | 8.1.*",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "infection/infection": "^0.20.2",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.2",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "~4.22.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": false
        }
    },
    "autoload": {
        "psr-4": {
            "Oqq\\Pact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oqq\\PactTest\\": "tests/"
        }
    }
}
